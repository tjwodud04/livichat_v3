{
  "version": 2,
  "includeFiles": [
    "bin/ffmpeg",
    "bin/ffprobe"
  ],
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "front/**",
      "use": "@vercel/static"
    },
    {
      "src": "model/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/model/(.*)",
      "dest": "/model/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/front/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/front/js/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/front/$1"
    }
  ]
}
